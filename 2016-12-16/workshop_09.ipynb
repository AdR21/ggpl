    "  [x[7],y[4],z[1]], #18\n",
    "  [x[4],y[4],z[1]], #19\n",
    "  [x[4],y[1],z[1]] #20\n",
    "]\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "cellsStruct = [\n",
    "  [1,2,12,11],\n",
    "  [2,3,13,12],\n",
    "  [3,4,14,13],\n",
    "  [4,5,15,14],\n",
    "  [5,6,16,15],\n",
    "  [6,7,17,16],\n",
    "  [7,8,18,17],\n",
    "  [8,9,19,18],\n",
    "  [9,10,20,19],\n",
    "  [10,1,11,20]\n",
    " \n",
    "]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "cellsStructBorder = [\n",
    "  [1,2],\n",
