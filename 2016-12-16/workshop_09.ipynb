    "]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "vertsStruct = [\n",
    "  [x[2],y[0],z[0]], #1 ---> external\n",
    "  [x[0],y[2],z[0]], #2\n",
    "  [x[0],y[6],z[0]], #3\n",
    "  [x[11],y[6],z[0]], #4\n",
    "  [x[11],y[2],z[0]], #5\n",
    "  [x[9],y[0],z[0]], #6\n",
    "  [x[6],y[0],z[0]], #7\n",
    "  [x[6],y[3],z[0]], #8\n",
    "  [x[5],y[3],z[0]], #9\n",
    "  [x[5],y[0],z[0]], #10\n",
    "  [x[3],y[1],z[1]], #11 ---> internal\n",
    "  [x[1],y[3],z[1]], #12\n",
    "  [x[1],y[5],z[1]], #13\n",
    "  [x[10],y[5],z[1]], #14\n",
