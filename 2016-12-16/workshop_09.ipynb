    "  19.0, #8\n",
    "  20.0, #9\n",
    "  21.0, #10\n",
    "  23.0  #11\n",
    "]\n",
    "\n",
    "y = [\n",
    "  1.0,  #0\n",
    "  3.0,  #1\n",
    "  4.0,  #2\n",
    "  5.0,  #3\n",
    "  7.0,  #4\n",
    "  10.0, #5\n",
    "  12.0  #6\n",
    "]\n",
    "\n",
    "z = [\n",
    "  0.0,  #0\n",
